<section id="cta" class="section cta">
  <div class="container text-center">
    <h2>{{ section_data.title or "¿Querés empezar?" }}</h2>
    <p>{{ section_data.text or "Completa el formulario y nos pondremos en contacto." }}</p>
    {% if section_data.ctas %}
      {% for cta in section.ctas %}
        <a href="{{ cta.url }}" class="btn-primary">{{ cta.text }}</a>
      {% endfor %}
    {% endif %}
  </div>
</section>
